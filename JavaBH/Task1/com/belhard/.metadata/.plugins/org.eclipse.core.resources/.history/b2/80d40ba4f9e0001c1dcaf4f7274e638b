package TasksStar;
import java.util.Scanner;

public class Task5B {

	public static void Main(String[] args) {
		 Scanner vvod = new Scanner(System.in);
	        for (int i = 0; true; i++) {
	            StringBuffer toll = new StringBuffer(vvod.nextLine());
	            toll.reverse();
	            System.out.println("Hello " + toll.toString());
	            vvod.close();
	        }
	}

}
